<p id="notice"><%= notice %></p>

<h1>Listing Tactics</h1>

<table>
  <thead>
    <tr>
      <th>Pitch</th>
      <th>Description</th>
      <th>Medium</th>
      <th>Importance</th>
      <th>Frequency</th>
      <th>Start</th>
      <th>End</th>
      <th>Kpi</th>
      <th>Budget</th>
      <th>Strategy</th>
      <th>Poc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tactics.each do |tactic| %>
      <tr>
        <td><%= tactic.pitch %></td>
        <td><%= tactic.description %></td>
        <td><%= tactic.medium %></td>
        <td><%= tactic.importance %></td>
        <td><%= tactic.frequency %></td>
        <td><%= tactic.start %></td>
        <td><%= tactic.end %></td>
        <td><%= tactic.kpi %></td>
        <td><%= tactic.budget %></td>
        <td><%= tactic.strategy %></td>
        <td><%= tactic.poc %></td>
        <td><%= link_to 'Show', tactic %></td>
        <td><%= link_to 'Edit', edit_tactic_path(tactic) %></td>
        <td><%= link_to 'Destroy', tactic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tactic', new_tactic_path %>
